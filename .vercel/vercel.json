{
  "buildCommand": "echo 'Prebuilt - skipping TypeScript build'",
  "outputDirectory": ".",
  "functions": {
    "dist/api/**/*.js": {
      "runtime": "vercel-node@3.0.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/api/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
