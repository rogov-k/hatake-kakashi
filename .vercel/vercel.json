{
  "buildCommand": "echo 'Skipping TypeScript build (already compiled)'",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-node@3.0.0"
    }
  },
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
