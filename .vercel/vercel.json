{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "echo 'Skipping TypeScript build (already compiled)'",
  "outputDirectory": ".",
  "builds": [
    { "src": "dist/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/cache", "dest": "/dist/api/cache.js" },
    { "src": "/api/echo", "dest": "/dist/api/echo.js" },
    { "src": "/api/paste", "dest": "/dist/api/paste.js" }
  ]
}
