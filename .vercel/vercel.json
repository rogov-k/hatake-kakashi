{
  "buildCommand": "echo 'Skipping TypeScript build (already compiled)'",
  "outputDirectory": ".",
  "builds": [
    { "src": "dist/api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/api/$1.js" },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ]
}
