{
  "buildCommand": "echo 'Skipping TypeScript build (already compiled)'",
  "outputDirectory": ".",
  "builds": [
    { "src": "/dist/*", "use": "@vercel/static" },
    { "src": "dist/functions/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/index.html" },
    { "src": "/favicon.ico", "dest": "/dist/favicon.ico" },
    { "src": "/api/cache", "dest": "/dist/functions/cache.js" },
    { "src": "/api/echo", "dest": "/dist/functions/echo.js" }
  ]
}
